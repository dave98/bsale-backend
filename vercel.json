{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "DB_DIALECT": "@db-dialect",
        "DB_HOST": "@db-host",
        "DB_PORT": "@db-port",
        "DB_USERNAME": "@db-username",
        "DB_PASSWORD": "@db-password",
        "DB_DATABASE": "@db-database"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}