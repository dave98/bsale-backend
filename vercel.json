{
    "version": 2,
    "env": {
        "DB_DIALECT": "@db_dialect",
        "DB_HOST": "@db_host",
        "DB_PORT": "@db_port",
        "DB_USERNAME": "@db_username",
        "DB_PASSWORD": "@db_password",
        "DB_DATABASE": "@db_database"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}