{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "DB_DIALECT": "@dbdialect",
        "DB_HOST": "@dbhost",
        "DB_PORT": "@dbport",
        "DB_USERNAME": "@dbusername",
        "DB_PASSWORD": "@dbpassword",
        "DB_DATABASE": "@dbdatabase"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}